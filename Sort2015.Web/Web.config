    <!--<add name="GLFeedContext" connectionString="Data Source=.\SQLEXPRESS;AttachDbFilename=|DataDirectory|glfeed.mdf;Integrated Security=True;MultipleActiveResultSets=True;Application Name=EntityFramework;User Instance=True"
      providerName="System.Data.SqlClient" />-->
